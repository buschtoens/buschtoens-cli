    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "prettier": "@clark/prettier-config/trailing-comma",
  "eslintConfig": {
    "extends": "./.eslintrc.cjs"
  }
}

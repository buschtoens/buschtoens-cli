{
  "name": "buschtoens",
  "version": "0.0.0",
  "description": "@buschtoens in your terminal!",
  "license": "MIT",
  "repository": "buschtoens/buschtoens-cli",
  "author": {
    "name": "Jan BuschtÃ¶ns",
    "email": "jan@buschtoens.me",
